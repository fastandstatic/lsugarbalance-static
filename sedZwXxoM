function handleSubmit(event) {
    event.preventDefault();
    event.stopPropagation();
    
     const data = new FormData(event.target);
    const value = Object.fromEntries(data.entries());
    const elements = event.target.elements;
    console.log("1")
    console.log(value);
    
    var obj ={};
    for(var i = 0 ; i < elements.length ; i++){
        var item = elements.item(i);
        obj[item.name] = item.value;
    }
    
    console.log(obj)


fetch("https://hook.us1.make.com/fq1rafv3wp0fexo7gnox9eq9nl3lkv7y",
    {
        body: JSON.stringify(value),
        headers: {
            Accept: 'application/json',
            'Content-Type': 'application/json',
        },
        method: "post",
       
    }) 
    .then((value) =>{
    
    
 console.log('todo ok');
            const numeromsg = document
                Swal.fire({
                    title: "Tu mensaje fue recibido, muy pronto te contactaremos",
                    icon: "success",
                    confirmButtonColor: "#000000",
                    confirmButtonText: "Cerrar",
                });
                
               // evento();
})
    .catch((error) => {
    console.error('Error:', error);
  });
  }

 
    
     
    function evento() {
        var evento= "formulario-enviado";
       fbq("trackCustom", evento), 
        gtag("event", evento, 
        { event_category: "Form", 
        event_label: evento });
    }

    function utm_llenar(){
        const valores = window.location.search;
        const urlParams = new URLSearchParams(valores);
        var term = urlParams.get('utm_term');
        var medium = urlParams.get('utm_medium');
        var campaign = urlParams.get('utm_campaign');
        var source = urlParams.get('utm_source');
        var content = urlParams.get('utm_content');
        document.getElementById("form-field-utm_term").value=term;
        document.getElementById("form-field-utm_medium").value=medium;
        document.getElementById("form-field-utm_campaign").value=campaign;
        document.getElementById("form-field-utm_source").value=source;
        document.getElementById("form-field-utm_content").value=content;
        //console.log("UTM LISTAS");
      }
 

    function addEvent() {
        utm_llenar();
        const form = document.querySelector('#formATS');
        console.log("form",form);
        if(!form){"No se encontro formulario con id"};
        form.addEventListener('submit', handleSubmit);
    }
  
    window.onload= addEvent();

</script>
